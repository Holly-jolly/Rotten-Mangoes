<div class="header">
  <h1>Rotten Mangoes </h1>
</div>
  <div class="home">
    <%= link_to "Back to Home", movies_path %><br/>
  </div>
    <h2>Log In </h2>
      <%= form_tag session_path do %>
        <div>
          <%= label_tag :email %><br/>
          <%= text_field_tag :email %>
        </div>
        <div>
          <%= label_tag :password %><br/>
          <%= password_field_tag :password %>
        </div>
          <div><%= submit_tag "Log In" %></div>
      <% end %>

