<%=form_for @user, url: admin_user_path do |f| %>
  <% if @user.errors.any? %>
    <div>
      <%= pluralize(@user.errors.count, "error") %> prevented the account from being edited:
      <ul>
      <% @user.errors.full_messages.each do |msg| %>
      <li><%= msg %></li>
      <% end %>
      </ul>
    </div>
  <% end %>
   <div>
      <%= f.label :email,"Email"%><br>
      <%= f.text_field :email %>
   </div>
    <div>
      <%= f.label :firstname, "First Name" %><br>
      <%= f.text_field :firstname %>
    </div>
    <div>
      <%= f.label :lastname, "Last Name" %><br>
      <%= f.text_field :lastname %>
    </div>
    
    <div><%= f.submit "Update" %></div>
<%end%>